@USEPROGRAMPREFIX_TRUE@uninstall-prefix-man:
@USEPROGRAMPREFIX_TRUE@	for f in $(PREFIXMAN1); do \
@USEPROGRAMPREFIX_TRUE@	  rm -f $(DESTDIR)$(man1dir)/$(NAMEPREFIX)`basename $$f`; \
@USEPROGRAMPREFIX_TRUE@	done
