font/devps/DESC: $(devps_srcdir)/DESC.in
	$(AM_V_GEN)$(MKDIR_P) font/devps \
	&& cat $(devps_srcdir)/DESC.in >$@ \
	&& echo broken $(BROKEN_SPOOLER_FLAGS) >>$@ \
	&& if test "$(PAGE)" = A4; then \
	     echo "papersize a4" >>$@; \
	   else \
	     echo "papersize letter" >>$@; \
	   fi \
	&& (test -z '$(PSPRINT)' \
	    || echo print '$(PSPRINT)' >>$@)

